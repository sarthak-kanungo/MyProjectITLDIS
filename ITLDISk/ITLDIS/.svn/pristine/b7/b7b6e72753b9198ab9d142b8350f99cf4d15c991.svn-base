<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE form-validation PUBLIC
          "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1.3//EN"
          "http://jakarta.apache.org/commons/dtds/validator_1_1_3.dtd">

<form-validation>

   <global>

 <formset>
      <form name="masterForm">

		 <field property="applicationCode" depends="required">

		 	<msg name="required" key="err.user.applicationCode.required" />
                        <arg name="required" key="${var:required}" position="0" resource="false"/>
                  <var>
            	        <var-name>required</var-name>
		        <var-value>applicationCode</var-value>
		</var>
		 </field>

     </form>
  </formset>





   </global>

</form-validation>
