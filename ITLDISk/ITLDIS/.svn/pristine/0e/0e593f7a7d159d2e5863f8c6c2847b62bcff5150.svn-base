	<queryString>
		<![CDATA[select umd.address,umd.CountryCode, umd.DealerName,con.CountryName, 
spi.PI_NO, spi.PI_DATE, spi.CONSIGNEE_NAME, spi.CONSIGNEE_ADDRESS, spi.DISCHARGE_PORT,
(select dp.dischargePortName from MSP_DischargePort_EXP dp  where dp.dischargePortCode=spi.DISCHARGE_PORT) as dischargePortName,
(select it.incoTermDesc from MSP_IncoTerms_EXP it where it.incoTermCode=spi.INCO_TERMS ) as IncoTermsDesc,
(select mpt.paymentTermDesc from MSP_PaymentTerms_EXP mpt where mpt.paymentTermCode=spi.PAYMENT_TERMS) as PaymentTermDesc,
spi.DESTINATION_PLACE,spi.PRE_CARRIAGE_BY, spi.PLACE_PRE_CARRIER,
delterm.DeliveryTermDesc, spid.ORD_PART_NO, spid.AVL_PART_NO,spid.LEAD_TIME,
(select cp.p1 from cat_part cp where cp.part_no=spid.ORD_PART_NO) as partDesc,
spid.FINAL_PI_QTY,spid.PRICE, spdtl.CUST_PO_NO, spdtl.LAST_UPDATED_ON
from UM_DealerMaster umd, 
MSP_Countries_EXP con, 
SP_ORD_PI_EXP spi, 
SP_ORD_PI_DTL_EXP spid, 
MSP_DeliveryTerms_EXP delterm,
SP_ORD_DTL_EXP spdtl
where umd.CountryCode=con.CountryCode 
and umd.DealerCode=spi.DEALER_CODE
and spi.DELIVERY_TERMS=delterm.DeliveryTermCode
and spdtl.PO_DTL_ID=spid.PO_DTL_ID
and spi.PI_NO=$P{piNo}]]>
	</queryString>
