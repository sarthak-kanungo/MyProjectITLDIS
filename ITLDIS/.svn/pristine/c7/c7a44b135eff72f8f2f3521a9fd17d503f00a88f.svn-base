div.suggestions {
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    border: 1px solid black;
    position: absolute;   
    background-color: white;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 10px;
    
}

div.suggestions div {
    cursor: default;
    padding: 0px 3px;
    font-family: Arial,Helvetica,sans-serif;
    
}

div.suggestions div.current {
    background-color: #3366cc;
    color: white;
}