spid.FINAL_PI_QTY, round(spid.PRICE,2) as price,
--(SELECT CURRENCY_TYPE FROM SP_PRICELIST_CODE SPC JOIN UM_USER_PRICELIST UMP ON 
--SPC.PRICELIST_CODE=UMP.PRICELIST_CODE
--where ump.USER_ID =(SELECT SOHE.Created_By FROM SP_ORD_HDR_EXP SOHE 
--WHERE SOHE.CUST_PO_NO =spo.CUST_PO_NO )) as currencyType , 
spi.PI_CURRENCY
from SP_ORD_PI_DTL_EXP spid,SP_ORD_PI_EXP spi, SP_ORD_DTL_EXP spo
where spi.PI_NO=spid.PI_NO 
and spo.PO_DTL_ID=spid.PO_DTL_ID
and spi.PI_NO=$P{piNo}]]>
