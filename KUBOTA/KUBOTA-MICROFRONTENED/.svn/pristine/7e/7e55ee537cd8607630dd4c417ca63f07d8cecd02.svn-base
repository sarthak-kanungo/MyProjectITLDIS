          </form>

        </div>
      </div>

    </div>
  </div>
</div>


<ngsw-search-table [tableTitle]="'Search Result'" [dataTable]="dataTable" [actionButtons]="actionButtons"
  [totalElements]="totalTableElements" [searchValue]="searchValue" [showPartial]="false"
  [clearSearchRow]="clearSearchRow"
  [clickOnTableFields]="[{title:'activeStatus',icon:'check_circle',iconClass:'material-icons'}]"
  (pageChange)="pageChange($event)">

  <td [ngsw-searchField]="activityType" [searchColumnName]="'activityType'">
    <input type="text" #activityType [(ngModel)]="activityTypeNg">
  </td>
  <td [ngsw-searchField]="budgetType" [searchColumnName]="'budgetType'">
    <input type="text" #budgetType [(ngModel)]="budgetTypeNg">
  </td>
  <td [ngsw-searchField]="maximumLimit" [searchColumnName]="'maximumLimit'">
    <input type="text" #maximumLimit [(ngModel)]="maximumLimitNg">
  </td>
  <td [ngsw-searchField]="maximumDayMonth" [searchColumnName]="'maximumDayMonth'">
    <input type="text" #maximumDayMonth [(ngModel)]="maximumDayMonthNg">
  </td>
  <td [ngsw-searchField]="kaiShare" [searchColumnName]="'kaiShare'">
    <input type="text" #kaiShare [(ngModel)]="kaiShareNg">
  </td>

  <td [ngsw-searchField]="activeStatus" [searchColumnName]="'activeStatus'">
    <input type="text" #activeStatus [(ngModel)]="activeStatusNg">
  </td>


</ngsw-search-table>