				            <input matInput #deliveryChallanNumber>
				        </mat-form-field>
				    </td>
				    <td [ngsw-searchField]="deliveryChallanDatePicker" [searchColumnName]="'deliveryChallanDate'">
				        <mat-form-field>
				            <input matInput [matDatepicker]="deliveryChallanDatePicker" placeholder="Choose a date" #deliveryChallanDate
				                (dateChange)="dateChanges($event, 'deliveryChallanDate')">
				            <mat-datepicker-toggle matSuffix [for]="deliveryChallanDatePicker"></mat-datepicker-toggle>
				            <mat-datepicker #deliveryChallanDatePicker></mat-datepicker>
				        </mat-form-field>
				    </td>
