<div class="container-fluid create-form  mt-4 mb-3">
        <div class="card-from mt-2">
            <div class="card">
                <div class="card-header">
                    <p class="mb-0">New Enquiry</p>
                </div>
                <div class="card-body">
                 <app-enquiry-creation-container (sendData)="getNewEnquiryFormData($event)"></app-enquiry-creation-container>
                 <app-enquiry-product-interested-container (sendDataWithFormName)="getProductInterestedFormData($event)"></app-enquiry-product-interested-container>
                 <app-prospect-details-container (sendProspectDetailsForm)="getProspectDetailsFormData($event)"></app-prospect-details-container>
                 <app-prospect-background-container (submitProsprctBackgroundForm)="getProspectBacgroundFormData($event)"></app-prospect-background-container>
                 <app-current-machine-details-container (submitCurrenMachineDetailsForm)="getCurrentMachineFormData($event)"></app-current-machine-details-container>
                 <app-retail-finance-container (saveRetailfinanceForm)="getRetailFinanceData($event)"></app-retail-finance-container>
                  
                    <div class=" text-center mt-2 ">
                        <button type="button" class="btn btn_primary px-3" (click)="validateForm('update')"> Update
                        </button>
                        <button type="button" class="btn btn_secondary px-3 ml-3"> Clear
                        </button>
                        <button type="button" class="btn btn_primary px-3 ml-3"> Exit
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>