<div class="container-fluid mt-4">
	<div class="container-fluid mb-3">
        <div class="row">
            <!-- <div class="col-lg-12"> -->
            <div class="card-from w-100">
            	<div class="card">
                       <div class="card-header d-flex justify-content-between header">
                               <p class="mb-0">Search Part Quotation</p>
                               <div>
                                   <button type="button" class="btn btn-xs ml-2 bg-white "
                                       [routerLink]="[ './create' ]">
                                       <b class="btnName">New</b>
                                   </button>
                               </div>
                       </div>
           			 <div class="card-body">
					    <form [formGroup]="searchForm">
					        <div class="row">
					            <div class="col-lg-12">
					                <div class="card-from">
					                    
					                        <app-part-quotation-search [partQuotationSearchForm]=partQuotationSearchForm>
					                        </app-part-quotation-search>
					
					                        <div class="text-center  mt-2 mb-2">
					                            <button type="button" class="btn  btn_primary px-3"
					                                (click)="onClickSearchPartQuotationDetails()" [insertQueryParams]="filterData"
					                                (initialQueryParams)="initialQueryParams($event)" (onUrlChange)="onUrlChange($event)">
					                                Search
					                            </button>
					                            <button type="button" class="btn  btn_primary px-3 ml-2"
					                                (click)="onClickClearPartQuotationDetails()">
					                                Clear
					                            </button>
					                        </div>
					
					                    </div>
					                </div>
					            </div>
					    	</form>
 					</div>
    			</div>
			</div>
		</div>
	</div>
    <ngsw-search-table [tableTitle]="'Search Result'" [dataTable]="dataTable" [totalElements]="totalTableElements"
        (actionOnTableRecord)="actionOnTableRecord($event)" [actionButtons]="actionButtons"
        [searchValue]="searchValue" (pageChange)="pageChange($event)"
        [clickOnTableFields]="[{ 'title': 'Quotation_Number', 'isClickable': true }, { 'title': 'Edit', 'isClickable': true }]" [clearSearchRow]="clearSearchRow" >

        <!-- <td [ngsw-searchField]="Edit" [searchColumnName]="'Edit'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #Edit [(ngModel)] ="EditNgModel">
            </mat-form-field>
        </td> -->
        <td></td>

        <td [ngsw-searchField]="Quotation_Number" [searchColumnName]="'Quotation_Number'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #Quotation_Number [(ngModel)] ="Quotation_NumberNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="Quotation_Date" [searchColumnName]="'Quotation_Date'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput [matDatepicker]="Quotation_Date" #Quotation_DateInput
                    (dateInput)="quotationDateChanges(Quotation_DateInput.value, 'Quotation_Date')" [(ngModel)] ="Quotation_DateNgModel">
                <mat-datepicker-toggle matSuffix [for]="Quotation_Date"></mat-datepicker-toggle>
                <mat-datepicker #Quotation_Date></mat-datepicker>
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="Customer_Type" [searchColumnName]="'Customer_Type'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #Customer_Type [(ngModel)] ="Customer_TypeNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="Customer_Name" [searchColumnName]="'Customer_Name'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #Customer_Name [(ngModel)] ="Customer_NameNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="Mobile_Number" [searchColumnName]="'Mobile_Number'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #Mobile_Number [(ngModel)] ="Mobile_NumberNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="Customer_Address1" [searchColumnName]="'Customer_Address1'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #Customer_Address1 [(ngModel)] ="Customer_Address1NgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="Customer_Address2" [searchColumnName]="'Customer_Address2'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #Customer_Address2 [(ngModel)] ="Customer_Address2NgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="Country" [searchColumnName]="'Country'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #Country [(ngModel)] ="CountryNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="State" [searchColumnName]="'State'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #State [(ngModel)] ="StateNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="District" [searchColumnName]="'District'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #District [(ngModel)] ="DistrictNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="Tehsil" [searchColumnName]="'Tehsil'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #Tehsil [(ngModel)] ="TehsilNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="City" [searchColumnName]="'City'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #City [(ngModel)] ="CityNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="Discount_Type" [searchColumnName]="'Discount_Type'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #Discount_Type [(ngModel)] ="Discount_TypeNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="Discount_Rate" [searchColumnName]="'Discount_Rate'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #Discount_Rate [(ngModel)] ="Discount_RateNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="Discount_Value" [searchColumnName]="'Discount_Value'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #Discount_Value [(ngModel)] ="Discount_ValueNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="Total_Basic_Value" [searchColumnName]="'Total_Basic_Value'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #Total_Basic_Value [(ngModel)] ="Total_Basic_ValueNgModel">
            </mat-form-field>
        </td>

    </ngsw-search-table>
</div>