<div class="container-fluid create-form mt-4 mb-3">
    <div class="card-from mt-2">
      <div class="card">
        <div class="card-header">
          <p class="mb-0">Re-Installation</p>
        </div>
        <div class="card-body">
          <form [formGroup]="reInstallationForm">
           <app-re-installation-details [reInstallationDetailsForm]=reInstallationDetailsForm (getEvent)="getCheckPoint($event)"></app-re-installation-details>
           <app-re-installtion-machine-details [machineDetailsTable]=machineDetailsTable  [viewReInstallation]="viewReInstallation"></app-re-installtion-machine-details>
           <div *ngIf="representativesDetailsdataTable.length > 0">
              <app-representative-details [representativesDetailsTable]=representativesDetailsTable></app-representative-details>
           </div>
           <div *ngIf="isSelectMachineSeries">
            <app-re-installation-checklist [reInstallationCheckPointListTable]=reInstallationCheckPointListTable [reInstallationCheckList]="reInstallationCheckList" 
            [ViewReInstallationCheckList]="ViewReInstallationCheckList"></app-re-installation-checklist>
           </div>
            <div class="text-center mt-2 ">
              <button type="button" class="btn btn_primary px-3" *ngIf="isShowBtn" (click)="saveAndSubmitForm(true)"  [disabled]="isSubmitDisable">
                Save
              </button>
              <button type="button" class="btn btn_primary px-3 ml-3" *ngIf="isShowBtn" (click)="saveAndSubmitForm(false)"  [disabled]="isSubmitDisable"> 
                Submit
              </button>
              <button type="button" class="btn btn_secondary px-3 ml-3" *ngIf="isShowBtn" disabled> Print
              </button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>