<div class="container-fluid create-form  mt-3 mb-3">
  <div class="heading">
    <p class="font-weight-bold">New Parts Claim Creation</p>
  </div>
  <div class="card-from">
    <div class="card">
      <div class="card-header">
        <p class="mb-0">New Parts Claim</p>
      </div>
      <div class="card-body">
        <form [style.fontSize.px]="10">
          <div class="row">
            <div class="col-lg-2">
              <mat-form-field appearance="outline" class="w-100">
                <mat-label>Claim/ Request Type</mat-label>
                <mat-select>
                  <mat-option *ngFor="let customer of customers" [value]="customer">{{customer}}</mat-option>
                </mat-select>
              </mat-form-field>
            </div>
            <div class="col-lg-2">
              <mat-form-field appearance="outline">
                <mat-label>Claim/ Request No.</mat-label>
                <input matInput placeholder="Claim/ Request No.">
                <mat-icon matSuffix>search</mat-icon>
              </mat-form-field>
            </div>

            <div class="col-lg-2">
              <mat-form-field appearance="outline" class="w-100">
                <input matInput [matDatepicker]="picker" placeholder="Claim/ Request Date" disabled>
                <mat-datepicker-toggle matSuffix [for]="picker"></mat-datepicker-toggle>
                <mat-datepicker #picker></mat-datepicker>
              </mat-form-field>
            </div>
            <div class="col-lg-2">
              <mat-form-field appearance="outline">
                <mat-label> Status </mat-label>
                <input matInput placeholder="Status" disabled>
              </mat-form-field>
            </div>
            <div class="col-lg-2">
              <mat-form-field appearance="outline">
                <mat-label>DMS GRN No.</mat-label>
                <input matInput placeholder="DMS GRN No.">
                <mat-icon matSuffix>search</mat-icon>
              </mat-form-field>
            </div>
            <div class="col-lg-2">
              <mat-form-field appearance="outline" class="w-100">
                <input matInput [matDatepicker]="picker1" placeholder="GRN Date" disabled>
                <mat-datepicker-toggle matSuffix [for]="picker1"></mat-datepicker-toggle>
                <mat-datepicker #picker1></mat-datepicker>
              </mat-form-field>
            </div>
            <div class="col-lg-2">
              <mat-form-field appearance="outline">
                <mat-label> Dealer Code </mat-label>
                <input matInput placeholder="Dealer Code" disabled>
              </mat-form-field>
            </div>
            <div class="col-lg-2">
              <mat-form-field appearance="outline">
                <mat-label> Mobile No. </mat-label>
                <input matInput placeholder="Mobile No." disabled>
              </mat-form-field>
            </div>
            <div class="col-lg-2">
              <mat-form-field appearance="outline">
                <mat-label> Claim Raised By </mat-label>
                <input matInput placeholder="Claim Raised By" disabled>
              </mat-form-field>
            </div>
            <div class="col-lg-2">
              <mat-form-field appearance="outline">
                <mat-label> No. of Boxes Sent </mat-label>
                <input matInput placeholder="No. of Boxes Sent" disabled>
              </mat-form-field>
            </div>
            <div class="col-lg-2">
              <mat-form-field appearance="outline">
                <mat-label>No. of Boxes Received </mat-label>
                <input matInput placeholder="No. of Boxes Received" disabled>
              </mat-form-field>
            </div>
            <div class="col-lg-2">
              <mat-form-field appearance="outline">
                <mat-label>KAI Invoice No </mat-label>
                <input matInput placeholder="KAI Invoice No">
              </mat-form-field>
            </div>
            <div class="col-lg-2">
              <mat-form-field appearance="outline" class="w-100">
                <input matInput [matDatepicker]="picker2" placeholder="Invoice Date" disabled>
                <mat-datepicker-toggle matSuffix [for]="picker2"></mat-datepicker-toggle>
                <mat-datepicker #picker2></mat-datepicker>
              </mat-form-field>
            </div>
            <div class="col-lg-2">
              <mat-form-field appearance="outline">
                <mat-label> Mode </mat-label>
                <input matInput placeholder="Mode" disabled>
              </mat-form-field>
            </div>
            <div class="col-lg-2">
              <mat-form-field appearance="outline">
                <mat-label> Transporter </mat-label>
                <input matInput placeholder="Transporter" disabled>
              </mat-form-field>
            </div>
            <div class="col-lg-2">
              <mat-form-field appearance="outline">
                <mat-label> LR No. </mat-label>
                <input matInput placeholder="LR No." disabled>
              </mat-form-field>
            </div>
          </div>
        </form>
        <app-new-parts-claim-table></app-new-parts-claim-table>
        <div class="row mt-2 mb-2">
          <div class="col-lg-3">
            <div class="d-flex bd-highlight">
              <div class="pr-1 w-25 flex-fill bd-highlight">
                <label class="col-form-label">Photo 1</label>
              </div>
              <div class="w-75 flex-fill bd-highlight">
                <div class="input-group">
                  <div class="custom-file">
                    <input type="file" class="custom-file-input" id="inputGroupFile04"
                      aria-describedby="inputGroupFileAddon04">
                    <label class="custom-file-label" for="inputGroupFile04">Choose file</label>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="row mt-2 mb-2">
          <div class="col-lg-3">
            <div class="d-flex bd-highlight">
              <div class="pr-1 w-25 flex-fill bd-highlight">
                <label class="col-form-label">Photo 2</label>
              </div>
              <div class="w-75 flex-fill bd-highlight">
                <div class="input-group">
                  <div class="custom-file">
                    <input type="file" class="custom-file-input" id="inputGroupFile04"
                      aria-describedby="inputGroupFileAddon04">
                    <label class="custom-file-label" for="inputGroupFile04">Choose file</label>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class=" text-center mt-2 ">
          <button type="button" class="btn btn_primary px-3 mx-2 ">
            Save
          </button>
          <button type="button" class="btn btn_primary px-3 mx-2 " (click)="validateForm()">
            Submit
          </button>
          <button type="button" class="btn btn_secondary px-3 mx-2 " disabled>
            View PDF
          </button>
          <button type="button" class="btn btn_secondary px-3 mx-2 " disabled>
            Print
          </button>
          <button type="button" class="btn btn_primary px-3 mx-2">
            Clear
          </button>
          <button type="button" class="btn btn_primary px-3 mx-2">
            Exit
          </button>
        </div>
        <div class="card-from mt-2">
          <div class="card" style="border: 1px solid #45afaf;">
            <div class="card-header" style="background-color: #45afaf;
              border: 1px solid #45afaf;">
              <p class="mb-0" style="font-size: 13px;">KAI Additional Remarks</p>
            </div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-bordered table-sm" style="overflow: hidden;">
                  <thead class="thead-light">
                    <!-- <tr>
                      <th scope="col" class="data2" style="padding-bottom: 11px;" colspan="2">KAI Additional Remarks
                      </th>
                      <th scope="col" class="data2" style="padding-bottom: 11px;" colspan="2">KAI Additional Remarks
                      </th>
                    </tr> -->
                  </thead>
                  <tbody>
                    <tr>
                      <td rowspan="2" colspan="2">
                        <div class="row">
                          <div class="col-lg-6">
                            <textarea class="form-control form-control-sm" placeholder="For Dealer" type="text"
                              style="height: 90px;"></textarea>
                          </div>
                          <div class="col-lg-6">
                              <textarea class="form-control form-control-sm" placeholder="Internal KAI" type="text"
                              style="height: 90px;"></textarea>
                          </div>
                        </div>

                      </td>
                      <td colspan="2">
                        <div class="row">
                          <div class="col-lg-6">
                            <input class="form-control form-control-sm" placeholder="Credit Note No." type="text">
                          </div>
                          <div class="col-lg-4">
                            <mat-form-field appearance="outline" class="w-100" [style.fontSize.px]="10">
                              <input matInput [matDatepicker]="picker5" placeholder=" Date" disabled>
                              <mat-datepicker-toggle matSuffix [for]="picker5"></mat-datepicker-toggle>
                              <mat-datepicker #picker5></mat-datepicker>
                            </mat-form-field>
                          </div>
                        </div>

                      </td>
                    </tr>

                    <tr>

                      <td>
                        <div class="row">
                          <div class="col-lg-6">
                            <input class="form-control form-control-sm" placeholder="Amount" type="text">
                          </div>
                        </div>

                      </td>
                    </tr>
                    <tr>
                      <td colspan="2">
                        <div class="row">
                          <div class="col-lg-6">
                            <button type="button" class="btn btn_primary px-3 mx-2 ">
                              KAI Submit Acceptance
                            </button>
                          </div>
                          <div class="col-lg-4">
                            <mat-form-field appearance="outline" class="w-100" [style.fontSize.px]="10">
                              <input matInput [matDatepicker]="picker3" placeholder=" Date" disabled>
                              <mat-datepicker-toggle matSuffix [for]="picker3"></mat-datepicker-toggle>
                              <mat-datepicker #picker3></mat-datepicker>
                            </mat-form-field>
                          </div>
                        </div>

                      </td>
                      <td colspan="2">
                        <div class="row">
                          <div class="col-lg-6">
                            <button type="button" class="btn btn_primary px-3 mx-2 ">
                              KAI Submit Settlement
                            </button>
                          </div>
                          <div class="col-lg-4">
                            <mat-form-field appearance="outline" class="w-100" [style.fontSize.px]="10">
                              <input matInput [matDatepicker]="picker4" placeholder=" Date" disabled>
                              <mat-datepicker-toggle matSuffix [for]="picker4"></mat-datepicker-toggle>
                              <mat-datepicker #picker4></mat-datepicker>
                            </mat-form-field>
                          </div>
                        </div>

                      </td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
