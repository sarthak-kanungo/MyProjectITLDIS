<div [formGroup]="surveySummaryReportForm" [style.fontSize.px]="11">
    <div class="row card-body">

        <div class="col-lg-2">
            <mat-form-field class="w-100" appearance="outline">
                <mat-label>Survey No</mat-label>
                <input matInput placeholder="Survey No" formControlName="surveyNo" [matAutocomplete]="autoSurveyNo">
                <mat-autocomplete #autoSurveyNo="matAutocomplete" [displayWith]="displayFnForSurveyNo"
                    [panelWidth]="200">
                    <mat-option *ngFor="let option of surveyNo" [value]="option">
                        {{option.Survey_No}}
                    </mat-option>
                </mat-autocomplete>
                <mat-icon matSuffix>search</mat-icon>
            </mat-form-field>
        </div>

         <div class="col-lg-2">
            <mat-form-field appearance="outline" class="w-100">
                <mat-label>Survey Type </mat-label>
                <mat-select formControlName="surveyType">
                    <mat-option *ngFor="let st of syurveyType" [value]="st.id">
                        {{st.name}}
                    </mat-option>
                </mat-select>
            </mat-form-field>
        </div>
        
        <div class="col-lg-2">
            <mat-form-field class="w-100" appearance="outline">
                <mat-label>Survey Name </mat-label>
                <input matInput placeholder="Survey Name " formControlName="surveyName">
            </mat-form-field>
        </div>

        <div class="col-lg-2">
            <mat-form-field appearance="outline" class="w-100">
                <mat-label>Survey Status </mat-label>
                <mat-select formControlName="surveyStatus">
                    <mat-option *ngFor="let surveyStatus of surveyStatusList?.result" [value]="surveyStatus?.lookupval">
                        {{surveyStatus?.lookupval}}
                    </mat-option>
                </mat-select>
            </mat-form-field>
        </div>

        <div class="col-lg-2" *ngIf="surveyType=='TELEPHONIC'">
            <mat-form-field appearance="outline" class="w-100">
                <mat-label>Date AS On </mat-label>
                <mat-select formControlName="asOnDate" (selectionChange)="selectionAsOnDate($event.value)">
                    <mat-option *ngFor="let surveyDate of surveyDateList?.result" [value]="surveyDate?.lookupval">
                        {{surveyDate?.lookupval}}
                    </mat-option>
                </mat-select>
            </mat-form-field>
        </div>

        <div class="col-lg-2" *ngIf="dateView">
            <mat-form-field class="w-100">
                <input matInput [matDatepicker]="picker" placeholder="From Date" formControlName="fromDate">
                <mat-datepicker-toggle matSuffix [for]="picker"></mat-datepicker-toggle>
                <mat-datepicker #picker></mat-datepicker>
            </mat-form-field>
        </div>

        <div class="col-lg-2" *ngIf="dateView">
            <mat-form-field class="w-100">
                <input matInput [matDatepicker]="picker1" [min]="surveySummaryReportForm.get('fromDate').value" placeholder="To Date" formControlName="toDate">
                <mat-datepicker-toggle matSuffix [for]="picker1"></mat-datepicker-toggle>
                <mat-datepicker #picker1></mat-datepicker>
            </mat-form-field>
        </div>

        <div class="col-lg-2">
            <mat-form-field class="w-100">
                <input matInput [matDatepicker]="picker" [max]="dcMaxDate"  placeholder="DC Cut Off From Date" formControlName="fromDcDate">
                <mat-datepicker-toggle matSuffix [for]="picker"></mat-datepicker-toggle>
                <mat-datepicker #picker></mat-datepicker>
            </mat-form-field>
        </div>

        <div class="col-lg-2">
            <mat-form-field class="w-100">
                <input matInput [matDatepicker]="picker1" [min]="surveySummaryReportForm.get('fromDcDate').value" [max]="dcMaxDate" placeholder="DC Cut off To Date" formControlName="toDcDate">
                <mat-datepicker-toggle matSuffix [for]="picker1"></mat-datepicker-toggle>
                <mat-datepicker #picker1></mat-datepicker>
            </mat-form-field>
        </div>

