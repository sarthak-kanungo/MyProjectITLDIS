<div class="container-fluid">
<div class="card-from">
    <div class="card">
        <div class="card-header">
            <div class="row">
                <div class="col-lg-12">
                    <div class="d-flex justify-content-between header">
                        <p class="mb-0 label"> Search Machine Transport</p>
                        <div>
                            <!-- <button type="button" class="btn btn-xs ml-2 bg-white " (click)="createActivityClaim()">
                                <b class="btnName">New</b>
                            </button> -->
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <div class="card-body">

            <form [formGroup]="searchmachineTransport" [style.fontSize.px]="10">
                <div class="row">
                    <div class="col-lg-2">
                        <mat-form-field appearance="outline">
                            <mat-label>Purchase Order No.</mat-label>
                            <input matInput placeholder="Purchase Order No." formControlName="poNumber"
                                [matAutocomplete]="poNumberAuto">
                            <mat-autocomplete #poNumberAuto="matAutocomplete"
                                [displayWith]="displayPoNumberFn" [panelWidth]="500">
                                <!-- <mat-option *ngFor="let option of poNumberAutoList | async"
                                    [value]="option.poNumber">
                                    {{option['suggestion']}}
                                </mat-option> -->
                            </mat-autocomplete>
                            <mat-icon matSuffix>search</mat-icon>
                            <!-- <mat-error
                                    *ngIf="searchPoForm.get('poNumber').hasError('required')">
                                    Allows only dropdown values</mat-error> -->
                        </mat-form-field>
                    </div>


                    <div class="col-lg-2">
                        <mat-form-field appearance="outline" class="w-100">
                            <mat-label>Purchase Status</mat-label> 
                            <mat-select formControlName="requestStatus">
                             
                            </mat-select>
                         </mat-form-field>
                    </div>
                    <div class="col-lg-2">
                        <mat-form-field class="w-100">
                            <input matInput [matDatepicker]="picker" placeholder="Purchase Order From Date"
                                formControlName="fromDate" (click)="picker.open()" 
                                (dateChange)="fromDateChange($event)">
                            <mat-datepicker-toggle matSuffix [for]="picker"></mat-datepicker-toggle>
                            <mat-datepicker #picker></mat-datepicker>
                        </mat-form-field>
                    </div>

                    <div class="col-lg-2">
                        <mat-form-field class="w-100">
                            <input matInput [matDatepicker]="picker1" placeholder="Purchase Order To Date"
                                formControlName="toDate" (click)="picker1.open()"
                                >
                            <mat-datepicker-toggle matSuffix [for]="picker1"></mat-datepicker-toggle>
                            <mat-datepicker #picker1></mat-datepicker>
                        </mat-form-field>
                    </div>


                  

                </div>
                <div class="col-lg-12 text-center">
                    <div>
                        <button type="button" class="btn pl-3 pr-3 btn_primary" (click)="searchData()"
                            [insertQueryParams]="searchFilter" (initialQueryParams)="initialQueryParams($event)"
                            (onUrlChange)="onUrlChange($event)"> Search </button>

                        <button type="button" class="btn pl-3 pr-3 ml-2 btn_primary" (click)="clear()"> Clear </button>
                    </div>
                </div>
            </form>

        </div>
    </div>

</div>

