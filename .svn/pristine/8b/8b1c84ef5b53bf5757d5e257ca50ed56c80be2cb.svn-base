<form>
    <div class="container-fluid create-form  mt-4 mb-3">
        <div class="row">
            <div class="col-lg-12">
                <div class="card-from">
                    <div class="card">
                        <div class="card-header">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="d-flex justify-content-between header">
                                        <p class="mb-0 label">Search Branch Transfer Reciept</p>
                                        <div>
                                            <button [routerLink]="[ './create' ]"
                                                class="font-weight-bold btn btn_header bg_white">
                                                New
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div>
                            <app-search-branch-transfer-reciept
                                [searchForm]="searchForm"></app-search-branch-transfer-reciept>
                        </div>
                        <div class=" text-center mt-2 mb-1">
                            <button type="button" class="btn  btn_primary" (click)="searchData()"
                                (initialQueryParams)="initialQueryParams($event)" (onUrlChange)="onUrlChange($event)">
                                Search
                            </button>
                            <button type="button" class="btn  btn_primary px-3 ml-2" (click)="clearForm()"> Clear
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
<div class="container-fluid">
    <ngsw-search-table [tableTitle]="'Search Result'" [dataTable]="dataTable" [actionButtons]="actionButtons"
        [totalElements]="totalSearchRecordCount" [searchValue]="searchValue" 
        (actionOnTableRecord)="actionOnTableRecord($event)" (pageChange)="pageChange($event)" [clickOnTableFields]="[
        { title: 'receiptNo', isClickable: true },{ title: 'action', isClickable: true }]"
        [clearSearchRow]="clearSearchRow">
        <td></td>

       
        
        <td [ngsw-searchField]="receiptNo" [searchColumnName]="'receiptNo'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #receiptNo [(ngModel)] ="receiptNoNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="issuingBranch" [searchColumnName]="'issuingBranch'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #issuingBranch [(ngModel)] ="issuingBranchNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="reqBy" [searchColumnName]="'reqBy'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #reqBy [(ngModel)] ="reqByNgModel">
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="receivingBranch" [searchColumnName]="'receivingBranch'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #receivingBranch [(ngModel)] ="receivingBranchNgModel">
            </mat-form-field>
        </td>
        


    </ngsw-search-table>
</div>