<div class="container-fluid mt-4">
    <div class="card-from">
        <div class="card">
            <div class="card-header d-flex justify-content-between">
                <p class="mb-0"> Search Order Planning Sheet</p>
                <button [routerLink]="[ './create' ]" class="font-weight-bold btn btn_header bg_white">
                    <b>New </b>
                </button>
            </div>
            <div class="card-body">

                <form [formGroup]="searchOrderPlanningSheet" [style.fontSize.px]="10">
                    <div class="row">
                        

                        <div class="col-lg-2">
                            <mat-form-field class="w-100" appearance="outline">
                                <mat-label>ORDER PLANNING SHEET NO</mat-label>
                                <input matInput placeholder="ORDER PLANNING SHEET NO" formControlName="opSheetNo" [matAutocomplete]="auto" 
                                >
                                <mat-icon matSuffix>search</mat-icon>
                                <mat-autocomplete autoActiveFirstOption #auto="matAutocomplete">
                                    <mat-option *ngFor="let option of sheetNumberData" [value]="option.opsNo">
                                      {{option.opsNo}}
                                    </mat-option>
                                  </mat-autocomplete>
                            </mat-form-field>
                        </div>

                        <div class="col-lg-2">
                            <mat-form-field appearance="outline" class="w-100">
                                <mat-label>Order Type</mat-label>
                                <mat-select  formControlName="orderType">
                                    <mat-option *ngFor="let order of orderTypeList" [value]="order.id">
                                        {{order.orderType}}
                                    </mat-option>
                                </mat-select>
                            </mat-form-field>
                        </div>

                        <div class="col-lg-2">
                            <mat-form-field appearance="outline" class="w-100">
                                <mat-label>Logic</mat-label>
                                <mat-select  formControlName="logic">
                                    <mat-option *ngFor="let option of logicData" [value]="option.id">
                                        {{option.logic}}
                                    </mat-option>
                                </mat-select>
                            </mat-form-field>
                        </div>



                         <div class="col-lg-3">
                            <mat-form-field class="w-100" appearance="outline">
                              <mat-label>Order Planning From Date</mat-label>
                              <input matInput [matDatepicker]="issueFromDatePicker" placeholder="Order Planning From Date" formControlName="fromDate" (dateChange)="fromDateSelected1($event)" >
                              <mat-datepicker-toggle matSuffix [for]="issueFromDatePicker"></mat-datepicker-toggle>
                              <mat-datepicker #issueFromDatePicker></mat-datepicker>
                            </mat-form-field>
                          </div> 
                      
                          <div class="col-lg-3">
                            <mat-form-field class="w-100" appearance="outline">
                              <mat-label>Order Planning To Date</mat-label>
                              <input matInput [matDatepicker]="issueToDatePicker" placeholder="Order Planning To Date" formControlName="toDate" [max]="maxDate" [min]="minDate">
                              <mat-datepicker-toggle matSuffix [for]="issueToDatePicker"></mat-datepicker-toggle>
                              <mat-datepicker #issueToDatePicker></mat-datepicker>
                            </mat-form-field>
                          </div> 



                    </div>

                </form>

                <div class="col-lg-6">
                    <div class="mx-auto d-flex justify-content-between">
                        <div></div>
                        <div>
                            <button type="button" class="btn px-3 mt-3 btn_primary"
                                (initialQueryParams)="initialQueryParams($event)" (onUrlChange)="onUrlChange($event)"
                                (click)="searchData()">
                                Search </button>
                            <button type="button" class="btn px-3 mt-3 ml-2 btn_primary" (click)="clearForm()">
                                Clear </button>
                        </div>

                    </div>
                </div>

            </div>

        </div>

    </div>

</div>

