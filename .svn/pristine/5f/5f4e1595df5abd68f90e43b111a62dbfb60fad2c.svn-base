<div class="container-fluid mt-4 mb-3">
    <form>
        <div class="row">
            <div class="col-lg-12">
                <div class="card-from">
                    <div class="card">
                        <div class="card-header">
                            <div class="d-flex justify-content-between header">
                                <p class="mb-0">Search Warranty Parts Delivery Challan</p>
                                <div>
                                  <button type="button" class="font-weight-bold btn btn_header bg_white btn-xs ml-2 "
                                    [routerLink]="['../../wcr-report/create']" routerLinkActive="router-link-active">
                                    <b class="btnName">New</b>
                                  </button>
                                </div>
                            </div>
                        </div>

                        <app-wpdc-search [searchForm]="searchForm"></app-wpdc-search>
                        <div class="text-center  mt-2 mb-2">
                            <button (click)="search()" [style.fontSize.px]="13" type="button" class="btn  btn_primary px-3" [insertQueryParams]="searchFilter" (initialQueryParams)="initialQueryParams($event)" (onUrlChange)="onUrlChange($event)"> Search
                            </button>
                            <button (click)="clear()" [style.fontSize.px]="13" type="button" class="btn  btn_primary px-3 ml-2"> Clear
                            </button>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </form>

    <!-- space for ngsw search table -->

    <div class="mt-2">
        <ngsw-search-table [tableTitle]="'Search Result'" [dataTable]="dataTable" [totalElements]="totalTableElements"
        (actionOnTableRecord)="tableAction($event)" [clickOnTableFields]="[
          { title: 'dcNo', isClickable: true }, { title: 'action', isClickable: true }
        ]" (pageChange)="pageSizeChange($event)" [page]="page" [size]="size" [searchValue]="searchValue" [actionButtons]="actionButtons" [clearSearchRow]="clearSearchRow" >
  
        <td [ngsw-searchField]="action" [searchColumnName]="'action'">
            <input class="w-100 borderRadius" #action />
        </td>
        <td [ngsw-searchField]="dcNo" [searchColumnName]="'dcNo'">
            <input class="w-100 borderRadius" #dcNo />
        </td>
        <td [ngsw-searchField]="dcDate" [searchColumnName]="'dcDate'">
            <input class="w-100 borderRadius" #dcDate
              type="date" (change)="etSearchDateValueChange(dcDate.value, 'dcDate')" />
        </td>
        <td [ngsw-searchField]="dealerName" [searchColumnName]="'dealerName'">
            <input class="w-100 borderRadius" #dealerName />
        </td>
        <td [ngsw-searchField]="dealerEmail" [searchColumnName]="'dealerEmail'">
            <input class="w-100 borderRadius" #dealerEmail />
        </td>
        <td [ngsw-searchField]="transporter_name" [searchColumnName]="'transporter_name'">
            <input class="w-100 borderRadius" #transporter_name />
        </td>
        <td [ngsw-searchField]="docketNumber" [searchColumnName]="'docketNumber'">
            <input class="w-100 borderRadius" #docketNumber />
        </td>
       
        <td [ngsw-searchField]="numberOfBox" [searchColumnName]="'numberOfBox'">
            <input class="w-100 borderRadius" #numberOfBox />
        </td>
       
        <td [ngsw-searchField]="dispatchDate" [searchColumnName]="'dispatchDate'">
            <input class="w-100 borderRadius" #dispatchDate
              type="date" (change)="etSearchDateValueChange(dispatchDate.value, 'dispatchDate')" />
        </td>
        <td [ngsw-searchField]="dealer_code" [searchColumnName]="'dealer_code'">
            <input class="w-100 borderRadius" #dealer_code />
        </td>
       
      </ngsw-search-table>
  
    </div>

</div>