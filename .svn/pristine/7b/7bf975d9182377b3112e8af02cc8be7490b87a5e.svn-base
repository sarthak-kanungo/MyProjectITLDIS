{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "kubota-salepresale": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
                "src/styles.scss",
                "./node_modules/bootstrap/scss/bootstrap.scss",
                "src/assets/scss/main.scss",
                "./node_modules/angular-slickgrid/lib/multiple-select/multiple-select.css",
                "./node_modules/angular-slickgrid/styles/css/slickgrid-theme-bootstrap.css"
              ],
              "scripts": [
            	  "./node_modules/jquery/dist/jquery.js",
                  "./node_modules/jquery-ui-dist/jquery-ui.min.js",  
              	"./node_modules/slickgrid/lib/jquery.event.drag-2.3.0.js",
                "./node_modules/bootstrap/dist/js/bootstrap.js",
              	"./node_modules/angular-slickgrid/lib/multiple-select/multiple-select.js"
              ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "kubota-salepresale:build",
            "port": 9001
          },
          "configurations": {
            "production": {
              "browserTarget": "kubota-salepresale:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "kubota-salepresale:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "kubota-salepresale:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "kubota-salepresale:serve:production"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/server",
            "main": "src/main.server.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": {
                "scripts": false,
                "styles": true
              }
            }
          }
        }
      }
    },
    "ngsw-search-table": {
      "projectType": "library",
      "root": "projects/ngsw-search-table",
      "sourceRoot": "projects/ngsw-search-table/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngsw-search-table/tsconfig.lib.json",
            "project": "projects/ngsw-search-table/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngsw-search-table/src/test.ts",
            "tsConfig": "projects/ngsw-search-table/tsconfig.spec.json",
            "karmaConfig": "projects/ngsw-search-table/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngsw-search-table/tsconfig.lib.json",
              "projects/ngsw-search-table/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "editable-table": {
      "projectType": "library",
      "root": "projects/editable-table",
      "sourceRoot": "projects/editable-table/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/editable-table/tsconfig.lib.json",
            "project": "projects/editable-table/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/editable-table/src/test.ts",
            "tsConfig": "projects/editable-table/tsconfig.spec.json",
            "karmaConfig": "projects/editable-table/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/editable-table/tsconfig.lib.json",
              "projects/editable-table/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "kubota-salepresale",
  "cli": {
    "analytics": "004f9def-08fb-4192-b6f8-77e33d8cae01"
  }
}