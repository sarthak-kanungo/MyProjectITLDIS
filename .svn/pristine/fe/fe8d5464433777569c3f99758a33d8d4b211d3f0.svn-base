{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
	"paths": {
        "ngsw-search-table": [
          "dist/ngsw-search-table"
        ],
        "ngsw-search-table/*": [
          "dist/ngsw-search-table/*"
        ],
        "editable-table": [
          "dist/editable-table"
        ],
        "editable-table/*": [
          "dist/editable-table/*"
        ]
      }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
