              <input matInput type="text" #isFinalApprovalStatus [(ngModel)] ="isFinalApprovalStatusNgModel">
            </mat-form-field>
          </td>
          <td [ngsw-searchField]="grpSeqNo" [searchColumnName]="'grpSeqNo'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
