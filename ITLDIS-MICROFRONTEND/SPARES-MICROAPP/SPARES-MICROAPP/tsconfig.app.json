{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
        "ngsw-search-table": [
          "dist/ngsw-search-table"
        ],
        "ngsw-search-table/*": [
          "dist/ngsw-search-table/*"
        ],
        "editable-table": [
            "dist/editable-table"
          ],
          "editable-table/*": [
            "dist/editable-table/*"
          ]
      }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
