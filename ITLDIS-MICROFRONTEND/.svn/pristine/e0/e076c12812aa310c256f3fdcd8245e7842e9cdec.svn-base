<form [formGroup]="machineStockSearchPageForm">
    <div class="container-fluid create-form  mt-4 mb-3">
        <div class="row">
            <div class="col-lg-12">
                <div class="card-from">
                    <div class="card">
                        <div class="card-header">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="d-flex justify-content-between header">
                                        <p class="mb-0 label">Search Machine Stock</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <app-machine-stock-search></app-machine-stock-search>
                        <div class=" text-center mt-2 mb-1">
                            <button type="button" class="btn  btn_primary" (click)="searchData()"> Search
                            </button>
                            <button type="button" class="btn  btn_primary px-3 ml-2" (click)="clearForm()"> Clear
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
<div class="container-fluid">
    <ngsw-search-table [tableTitle]="'Search Result'" [dataTable]="dataTable" [totalElements]="totalSearchRecordCount"
        [searchValue]="searchValue" [clearSearchRow]="clearSearchRow" (pageChange)="pageChange($event)" [isExcelImport]="true" (exportToExcel)="machineStockExcelReport($event)">

        <td [ngsw-searchField]="zone" [searchColumnName]="'zone'" *ngIf="userType==='kubota'">
            <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                <input matInput type="text" #zone>
            </mat-form-field>
        </td>
        <td [ngsw-searchField]="region" [searchColumnName]="'region'" *ngIf="userType==='kubota'">
