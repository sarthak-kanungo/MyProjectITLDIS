<div class="card-from mt-2">
    <div class="card">
        <div class="card-header">
            <p class="mb-0">Customer Details</p>
        </div>
        <div class="card-body">
            <div [formGroup]="directSurveyCreateCustomerDetailsForm" [style.fontSize.px]="11">
                <div class="row">
                      <!-- <div class="col-lg-2">
                        <mat-form-field class="w-100" appearance="outline">
                          <mat-label>Customer Code/Mobile No.</mat-label>
                          <input matInput placeholder="Customer Code/Mobile No." required minlength="10" maxlength="10"
                            formControlName="mobileNumber"  [matAutocomplete]="autoMobileNo">
