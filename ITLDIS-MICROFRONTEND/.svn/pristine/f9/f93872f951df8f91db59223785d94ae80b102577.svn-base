    <div class="card-from">

        <div class="card">
            <div class="card-header">
                <p class="mb-0">Binning Slip</p>
            </div>
            <div class="card-body">

                <form [style.fontSize.px]="11">
                    <div class="row">

                            <div class="col-lg-2">
                                    <mat-form-field appearance="outline">
                                      <mat-label>Supplier Invoice No.</mat-label>
                                      <input matInput placeholder="Supplier Invoice No.">
                                      <mat-icon matSuffix>search</mat-icon>
                                    </mat-form-field>
                                  </div>

                        <div class="col-lg-2">
                            <mat-form-field appearance="outline">
                              <mat-label>GRN No.</mat-label>
                              <input matInput placeholder="GRN No.">
                              <mat-icon matSuffix>search</mat-icon>
                            </mat-form-field>
                          </div>

                          
                          <div class="col-lg-2">
                            <mat-form-field appearance="outline">
                              <mat-label>Dealer Code</mat-label>
                              <input matInput placeholder="Dealer Code">
                              <mat-icon matSuffix>search</mat-icon>
                            </mat-form-field>
                          </div> 

                              <div class="col-lg-2">
                                  <mat-form-field appearance="outline" class="w-100">
                                      <mat-label>GRN Type</mat-label>
                                      <mat-select formControlName="grnType">
                                          <mat-option *ngFor="let grnType of grnTypes" [value]="grnType">
                                              {{grnType}}
                                          </mat-option>
                                      </mat-select>
                                  </mat-form-field>
                              </div>

                           
                          <div class="col-lg-2">
                            <mat-form-field class="w-100">
                                <input matInput [matDatepicker]="picker" placeholder="GRN From Date">
                                <mat-datepicker-toggle matSuffix [for]="picker"></mat-datepicker-toggle>
                                <mat-datepicker #picker></mat-datepicker>
                            </mat-form-field>
                        </div>

                        <div class="col-lg-2">
                            <mat-form-field class="w-100" >
                                <input matInput [matDatepicker]="picker1" placeholder="GRN To Date">
                                <mat-datepicker-toggle matSuffix [for]="picker1"></mat-datepicker-toggle>
                                <mat-datepicker #picker1></mat-datepicker>
                            </mat-form-field>
                        </div>
              
                          <div class="col-lg-12 text-center">
                              <div class="mt-1">
                                  <button type="button" class="btn pl-3 pr-3 btn_primary" > Search </button>
                              </div>
                          </div>
            
                    </div>
                </form>

            </div>
        </div>
    </div>
