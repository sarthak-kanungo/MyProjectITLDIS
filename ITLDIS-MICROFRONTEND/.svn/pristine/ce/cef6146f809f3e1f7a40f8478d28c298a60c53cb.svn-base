<div class="card-from mt-2 mb-2">
    <div class="card" style="border: 1px solid #45afaf;">
        <div class="card-header" style="background-color: #45afaf;
                                        border: 1px solid #45afaf;">
            <p class="mb-0" style="font-size: 13px;">Customer Concerns</p>
        </div>
        <div class="card-body">
            <div [formGroup]="customerConcernForm" [style.fontSize.px]="11">


                <div class="row">

                    <div class="col-lg-4">
                        <mat-form-field appearance="outline">
                            <mat-label>Customer Concern</mat-label>
                            <input matInput placeholder="Customer Concern"
                                formControlName="customerConcernEditableTable">
                        </mat-form-field>
                    </div>
                    <div class="col-lg-2">
                        <mat-form-field appearance="outline" class="w-100">
                            <mat-label>Service Representative</mat-label>
                            <mat-select formControlName="serviceRepresentative"
                                [compareWith]="serviceRepresentativeCompare">
                                <mat-option *ngFor="let servicerepresentive of serviceRepresentives"
                                    [value]="servicerepresentive">
                                    {{servicerepresentive.name}}
                                </mat-option>
                            </mat-select>
                        </mat-form-field>
                    </div>

                    <div class="col-lg-2">
                        <mat-form-field class="w-100" appearance="outline">
                            <mat-label>Estimated Completion Date</mat-label>
                            <input matInput [matDatepicker]="picker5" readonly placeholder="Estimated Completion Date"
                                [min]="dateInValue" formControlName="estimatedCompletionDate" (click)="picker5.open()">
                            <mat-datepicker-toggle matSuffix [for]="picker5"></mat-datepicker-toggle>
                            <mat-datepicker #picker5></mat-datepicker>
                        </mat-form-field>
                    </div>

                    <div class="col-lg-2">
                        <mat-form-field appearance="outline">
                            <mat-label>Estimated Amount</mat-label>
                            <input matInput placeholder="Estimated Amount" formControlName="estimatedAmount">
                        </mat-form-field>
                    </div>
                </div>
            </div>


        </div>

    </div>
</div>

