<div class="container-fluid mt-3">
        <div class="row" >
          <div class="col-lg-12">
            <div class="d-flex justify-content-between">
              <div>
                <p class="font-weight-bold" style="color: #354052;">
                  Transporter Master
                </p>
              </div>
              <div>
                <a [routerLink]="[ './create' ]" class="font-weight-bold">
                  <u>Create Transporter Master </u>
                </a>
              </div>
            </div>
      
          </div>

          <div class="col-lg-12">
              <div class="card-from">
        
                <div class="card">
                  <div class="card-header">
                    <p class="mb-0"> Search Transporter</p>
                  </div>
                  <div class="card-body">
        
                    <form [formGroup]="transporterForm" [style.fontSize.px]="11">
                      <div class="row">
                          <div class="col-lg-2">
                              <mat-form-field appearance="outline" class="w-100">
                                <mat-label>Transporter Code</mat-label>
                                <input matInput placeholder="Transporter Code" formControlName="transporterCode">
                                <mat-icon matSuffix>search</mat-icon>
                              </mat-form-field>
                            </div>
                          <div class="col-lg-2">
                              <mat-form-field appearance="outline" class="w-100">
                                <mat-label>Transporter Name</mat-label>
                                <input matInput placeholder="Transporter Name" formControlName="transporterName">
                                <mat-icon matSuffix>search</mat-icon>
                              </mat-form-field>
                            </div>
                        <div class="col-lg-2">
                            <div>
                                <button type="button" class="btn pl-3 pr-3 btn_primary" > Search </button>
                            </div>
                        </div>
        
                      </div>
                     
                    </form>
        
                  </div>
                </div>
        
              </div>
            </div>

        </div>

        <app-transporter-master-search-result></app-transporter-master-search-result>
      
       
      </div>