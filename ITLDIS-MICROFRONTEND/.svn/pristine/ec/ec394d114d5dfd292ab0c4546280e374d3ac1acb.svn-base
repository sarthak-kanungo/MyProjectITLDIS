<div class="container-fluid create-form mt-4 mb-3">
    <div class="card-from mt-2">
      <div class="card">
        <div class="card-header">
          <p class="mb-0">Part Quotation</p>
        </div>
        <div class="card-body">
          <form [formGroup]="partQuotationForm">
           <app-part-quotation [partQuotationDetailsForm]=partQuotationDetailsForm [headerData]="headerData" [markForCheck]="markForCheck"></app-part-quotation>
           <app-part-quotation-table [partQuotationTotalForm]=partQuotationTotalForm [viewItemDeatils]="viewItemDeatils" [markForCheck]="markForCheck"></app-part-quotation-table>
           <div class=" text-center mt-2 ">
            <button type="button" class="btn btn_primary px-3 mx-2 " *ngIf="!isView" (click)="saveAndSubmitQuotation(true)"  [disabled]="isSubmitDisable">
              Save
            </button>
            <button type="button" class="btn btn_primary px-3 mx-2 " *ngIf="!isView" (click)="saveAndSubmitQuotation(false)"  [disabled]="isSubmitDisable">
              Submit
            </button>
           <button type="button" class="btn btn_secondary px-3 mx-2 " *ngIf="isView"  (click)="viewPrint('true')">
              View PDF
            </button>
           <!--   <button type="button" class="btn btn_secondary px-3 mx-2 " *ngIf="!isView" disabled >
              Generate Sales Order
            </button> -->
            <!-- <button type="button" class="btn btn_primary px-3 mx-2" *ngIf="!isView && !isEdit" (click)="onClickClear()">
              Clear
            </button> -->
            <button type="button" class="btn btn_primary px-3 mx-2" (click)="exitForm()">
              Exit
            </button>
          </div>
          </form>
        </div>
      </div>
    </div>
  </div>