<div class="container-fluid mt-3 mb-3">
    <div class="row">
      <div class="col-lg-12">
        <div class="d-flex justify-content-between">
          <div>
            <p class="font-weight-bold" style="color: #354052">
              KAI Branch/ Depot Master
            </p>
          </div>
  
          <div><a [routerLink]="[ './create' ]"
              class="font-weight-bold">
              <u>Create KAI Branch/ Depot Master </u>
            </a></div>
  
        </div>
  
      </div>
  
      <div class="col-lg-12">
        <div class="card-from">
  
          <div class="card">
            <div class="card-header">
              <p class="mb-0">Search KAI Branch/ Depot</p>
            </div>
            <div class="card-body">
  
              <form [formGroup]="kaiBranchDepotForm" [style.fontSize.px]="11">
                <div class="row">
  
  
                  <div class="col-lg-2 ">
                    <mat-form-field appearance="outline" class="w-100">
                      <mat-label>Branch Code</mat-label>
                      <mat-select formControlName="branchCode">
                        <mat-option *ngFor="let branchCode of branchCodes" [value]="branchCode">{{branchCode}}
                        </mat-option>
                      </mat-select>
                    </mat-form-field>
                  </div>
  
                  <div class="col-lg-2 ">
                    <mat-form-field appearance="outline" class="w-100">
                      <mat-label>Depot</mat-label>
                      <mat-select formControlName="depot">
                        <mat-option *ngFor="let depot of depots" [value]="depot">{{depot}}</mat-option>
                      </mat-select>
                    </mat-form-field>
                  </div>
  
                  <div class="col-lg-2">
                    <div class="mt-1">
                      <button type="button" class="btn pl-3 pr-3 btn_primary"> Search </button>
                    </div>
                  </div>
  
                </div>
  
              </form>
  
            </div>
          </div>
  
        </div>
      </div>
    </div>
  
  <app-kai-branch-depot-search-result></app-kai-branch-depot-search-result>
  </div>