<form [formGroup]="currentStockSearchResultForm">
    <div class="container-fluid create-form  mt-4 mb-3">
        <div class="row">
            <div class="col-lg-12">
                <div class="card-from">
                    <div class="card">
                        <div class="card-header">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="d-flex justify-content-between header">
                                        <p class="mb-0 label">Search Current Stock Details</p>
                                        <div>
                                            <!-- <button [routerLink]="[ './create' ]"
                                                class="font-weight-bold btn btn_header bg_white">
                                                New
                                            </button> -->
                                            <button type="button"  class="font-weight-bold btn btn_header bg_white"
                                            (click)="downloadExcel($event)"> Current Stock
                                        </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <app-current-stock-search [currentStockSearchResultForm]=currentStockSearchResultForm></app-current-stock-search>
                        <div class=" text-center mt-2 mb-1">
                            <button type="button" class="btn  btn_primary" (click)="searchData()"
                                [insertQueryParams]="filterData" (initialQueryParams)="initialQueryParams($event)"
                                (onUrlChange)="onUrlChange($event)"> Search
                            </button>
                           
                            <button type="button" class="btn  btn_primary px-3 ml-2" (click)="clearForm()"> Clear
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <form>
        <div class="container-fluid">
            <ngsw-search-table [tableTitle]="'Search Result'" [dataTable]="dataTable" [actionButtons]="actionButtons"
                [totalElements]="totalSearchRecordCount" [searchValue]="searchValue" [page]="page" [size]="size"
                (actionOnTableRecord)="actionOnTableRecord($event)" (pageChange)="pageChangeOfSearchTable($event)"
                [clickOnTableFields]="[{ 'title': 'print', 'isClickable': true }]" [clearSearchRow]="clearSearchRow" >

                <td [ngsw-searchField]="itemNo" [searchColumnName]="'itemNo'">
                    <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                        <input matInput type="text" #itemNo>
                    </mat-form-field>
                </td>
                <td [ngsw-searchField]="itemDescription" [searchColumnName]="'itemDescription'">
                    <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                        <input matInput type="text" #itemDescription>
                    </mat-form-field>
                </td>
                <td [ngsw-searchField]="binLocation" [searchColumnName]="'binLocation'">
                    <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                        <input matInput type="text" #binLocation>
                    </mat-form-field>
                </td>
                <td [ngsw-searchField]="storeName" [searchColumnName]="'storeName'">
                    <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                        <input matInput type="text" #storeName>
                    </mat-form-field>
                </td>
                <td [ngsw-searchField]="spmrp" [searchColumnName]="'spmrp'">
                    <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                        <input matInput type="text" #spmrp>
                    </mat-form-field>
                </td>
                <td [ngsw-searchField]="availableStock" [searchColumnName]="'availableStock'">
                    <mat-form-field class="w-100 pb_0" appearance="outline" [style.fontSize.px]="10">
                        <input matInput type="text" #availableStock>
                    </mat-form-field>
                </td>
            </ngsw-search-table>
        </div>