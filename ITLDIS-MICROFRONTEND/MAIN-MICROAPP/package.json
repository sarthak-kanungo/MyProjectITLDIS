{
  "name": "itldis-main",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng build editable-table && ng build ngsw-search-table && ng serve --poll 200 --port 9000",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run itldis-main:server:production --bundleDependencies all",
    "ng-high-memory": "node --max_old_space_size=4098 node_modules/@angular/cli/bin/ng",
    "high-memory-build:client": "npm run ng-high-memory build --prod",
    "high-memory-build:client-and-server-bundles": "npm run ng-high-memory build --prod && npm run ng-high-memory run itldis-main:server:production",
    "high-memory-build:ssr": "npm run build-libraries && npm run high-memory-build:client-and-server-bundles && npm run compile:server",
    "high-memory-build:staging-and-server-bundles": "npm run ng-high-memory build --configuration=staging && npm run ng-high-memory run itldis-main:server:staging",
    "build-staging:ssr": "npm run high-memory-build:staging-and-server-bundles && npm run compile:server",
    "build-libraries": "ng build ngsw-search-table && ng build editable-table"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/platform-server": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ng-bootstrap/ng-bootstrap": "^5.1.0",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@ngxs/store": "^3.5.1",
    "angular-user-idle": "^2.2.6",
    "bootstrap": "^4.3.1",
    "compression": "^1.7.4",
    "crypto-js": "^4.2.0",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "ng2-pdf-viewer": "^6.0.2",
    "ngx-image-compress": "^8.0.3",
    "ngx-toastr": "^11.0.0",
    "ngx-ui-loader": "^8.0.0",
    "object-to-formdata": "^2.1.2",
    "rxjs": "~6.5.3",
    "rxjs-compat": "^6.5.3",
    "tslib": "^1.9.0",
    "uuid": "^3.3.3",
    "xlsx": "^0.15.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.29",
    "@angular-devkit/build-ng-packagr": "^0.803.29",
    "@angular/cli": "^8.3.29",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^5.7.1",
    "protractor": "^5.4.4",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.3.12"
  }
}
